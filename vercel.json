{
  "functions": {
    "api/webhook.ts": {
      "maxDuration": 60
    },
    "api/health.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/webhook",
      "destination": "/api/webhook"
    },
    {
      "source": "/healthcheck",
      "destination": "/api/health"
    }
  ]
}
